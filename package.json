{
  "name": "findmydoc-portal",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate": "pnpm run generate:importmap && pnpm run payload generate:types",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "check": "cross-env NODE_OPTIONS=--no-deprecation next lint && tsc --noEmit",
    "test": "cross-env NODE_OPTIONS=--no-deprecation echo 'No tests implemented yet'",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "ci": "cross-env NODE_OPTIONS=--no-deprecation bash -c 'if [ \"$DB_FRESH\" = \"true\" ]; then pnpm run reset:db; else pnpm run migrate; fi && pnpm build'",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "reset:db": "pnpm run payload migrate:fresh",
    "migrate": "pnpm run payload migrate",
    "generateDBFromScratch": "cross-env NODE_OPTIONS=--no-deprecation find src/migrations -type f -delete && pnpm run generate:types && pnpm run generate:importmap && payload migrate:create --skip-empty && pnpm run reset:db",
    "vercel:preview": "vercel build --target=preview"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.42.0",
    "@payloadcms/live-preview-react": "3.42.0",
    "@payloadcms/next": "3.42.0",
    "@payloadcms/payload-cloud": "3.42.0",
    "@payloadcms/plugin-form-builder": "3.42.0",
    "@payloadcms/plugin-nested-docs": "3.42.0",
    "@payloadcms/plugin-redirects": "3.42.0",
    "@payloadcms/plugin-search": "3.42.0",
    "@payloadcms/plugin-seo": "3.42.0",
    "@payloadcms/richtext-lexical": "3.42.0",
    "@payloadcms/storage-s3": "^3.42.0",
    "@payloadcms/ui": "3.42.0",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.12",
    "@supabase/ssr": "^0.6.1",
    "@supabase/supabase-js": "^2.50.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.513.0",
    "next": "^15.4.0",
    "next-sitemap": "^4.2.3",
    "payload": "3.42.0",
    "payload-admin-bar": "^1.0.7",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "7.57.0",
    "react-icons": "^5.5.0",
    "sharp": "0.34.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.64"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.10",
    "@tailwindcss/typography": "^0.5.16",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.15.29",
    "@types/react": "19.1.7",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "15.3.3",
    "postcss": "^8.5.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "5.8.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  },
  "packageManager": "pnpm@10.10.0"
}
