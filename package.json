{
  "name": "findmydoc-portal",
  "version": "1.0.0",
  "description": "International patient clinic discovery platform built with PayloadCMS and Next.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev --turbopack",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate": "pnpm run generate:importmap && pnpm run payload generate:types",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation eslint --ext .ts,.tsx,.js,.jsx ./src ./tests ./scripts --cache",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation eslint --ext .ts,.tsx,.js,.jsx ./src ./tests ./scripts --fix",
    "check": "cross-env NODE_OPTIONS=--no-deprecation pnpm run lint && tsc --noEmit",
    "tests": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=test vitest run",
    "tests:show-logs": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=test TEST_SHOW_LOGS=1 vitest run",
    "tests:watch": "cross-env NODE_OPTIONS=--no-deprecation vitest",
    "matrix:verify": "pnpm dlx tsx scripts/permission-matrix/verify.ts",
    "matrix:derive": "pnpm dlx tsx scripts/permission-matrix/derive-json.ts",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "ci": "cross-env NODE_OPTIONS=--no-deprecation bash -c 'if [ \"$DB_FRESH\" = \"true\" ]; then pnpm run payload migrate:fresh; else pnpm run migrate; fi && pnpm build'",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "migrate": "pnpm run payload migrate",
    "generateDBFromScratch": "cross-env NODE_OPTIONS=--no-deprecation find src/migrations -type f -delete && pnpm run generate && payload migrate:create --skip-empty && pnpm run payload migrate:fresh",
    "vercel:preview": "vercel build --target=preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.68.4",
    "@payloadcms/live-preview-react": "3.68.4",
    "@payloadcms/next": "3.68.4",
    "@payloadcms/payload-cloud": "3.68.4",
    "@payloadcms/plugin-form-builder": "3.68.4",
    "@payloadcms/plugin-import-export": "3.68.4",
    "@payloadcms/plugin-nested-docs": "3.68.4",
    "@payloadcms/plugin-redirects": "3.68.4",
    "@payloadcms/plugin-search": "3.68.4",
    "@payloadcms/plugin-seo": "3.68.4",
    "@payloadcms/richtext-lexical": "3.68.4",
    "@payloadcms/storage-s3": "3.68.4",
    "@payloadcms/ui": "3.68.4",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.86.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^10.1.0",
    "graphql": "^16.12.0",
    "lucide-react": "^0.556.0",
    "next": "^15.5.9",
    "next-sitemap": "^4.2.3",
    "payload": "3.68.4",
    "payload-admin-bar": "^1.0.7",
    "posthog-js": "^1.302.2",
    "posthog-node": "^5.17.2",
    "react": "^19.2.1",
    "react-dom": "^19.2.1",
    "react-hook-form": "7.68.0",
    "sharp": "0.34.5",
    "tailwind-merge": "^3.4.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@eslint/css": "^0.14.1",
    "@eslint/eslintrc": "^3.3.3",
    "@next/eslint-plugin-next": "16.0.10",
    "@poupe/eslint-plugin-tailwindcss": "^0.2.4",
    "@radix-ui/react-slider": "^1.3.6",
    "@storybook/addon-a11y": "^10.1.8",
    "@storybook/addon-docs": "^10.1.8",
    "@storybook/addon-onboarding": "^10.1.8",
    "@storybook/addon-vitest": "^10.1.8",
    "@storybook/jest": "^0.2.3",
    "@storybook/nextjs-vite": "^10.1.8",
    "@storybook/react": "^10.1.8",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "24.10.1",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitest/browser-playwright": "^4.0.15",
    "@vitest/coverage-v8": "^4.0.15",
    "@vitest/ui": "4.0.15",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.0.10",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.8",
    "pg": "^8.16.3",
    "playwright": "^1.57.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "shadcn": "^3.6.1",
    "storybook": "^10.1.8",
    "tailwindcss": "^4.1.17",
    "ts-node": "^10.9.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.49.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@poupe/eslint-plugin-tailwindcss",
