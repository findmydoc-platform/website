{
  "name": "findmydoc-portal",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=\"${NODE_OPTIONS} --no-deprecation\" next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate": "pnpm run generate:importmap && pnpm run payload generate:types",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "check": "cross-env NODE_OPTIONS=--no-deprecation next lint && tsc --noEmit",
    "tests": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=test vitest run",
    "tests:watch": "cross-env NODE_OPTIONS=--no-deprecation vitest",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "ci": "cross-env NODE_OPTIONS=--no-deprecation bash -c 'if [ \"$DB_FRESH\" = \"true\" ]; then pnpm run reset:db; else pnpm run migrate; fi && pnpm build'",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "reset:db": "pnpm run payload migrate:fresh",
    "migrate": "pnpm run payload migrate",
    "generateDBFromScratch": "cross-env NODE_OPTIONS=--no-deprecation find src/migrations -type f -delete && pnpm run generate:types && pnpm run generate:importmap && payload migrate:create --skip-empty && pnpm run reset:db",
    "vercel:preview": "vercel build --target=preview"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.54.0",
    "@payloadcms/live-preview-react": "3.54.0",
    "@payloadcms/next": "3.54.0",
    "@payloadcms/payload-cloud": "3.54.0",
    "@payloadcms/plugin-form-builder": "3.54.0",
    "@payloadcms/plugin-import-export": "^3.54.0",
    "@payloadcms/plugin-nested-docs": "3.54.0",
    "@payloadcms/plugin-redirects": "3.54.0",
    "@payloadcms/plugin-search": "3.54.0",
    "@payloadcms/plugin-seo": "3.54.0",
    "@payloadcms/richtext-lexical": "3.54.0",
    "@payloadcms/storage-s3": "3.54.0",
    "@payloadcms/ui": "3.54.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^10.0.0",
    "geist": "^1.5.1",
    "graphql": "^16.11.0",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.542.0",
    "next": "^15.5.2",
    "next-sitemap": "^4.2.3",
    "payload": "3.54.0",
    "payload-admin-bar": "^1.0.7",
    "posthog-js": "^1.261.7",
    "posthog-node": "^5.8.2",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "7.62.0",
    "react-icons": "^5.5.0",
    "sharp": "0.34.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@tailwindcss/postcss": "^4.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "24.4.0",
    "@types/pg": "^8.15.5",
    "@types/react": "19.1.13",
    "@types/react-dom": "19.1.9",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "^10.4.21",
    "copyfiles": "^2.4.1",
    "dotenv": "^17.2.2",
    "eslint": "^9.35.0",
    "eslint-config-next": "15.5.3",
    "pg": "^8.16.3",
    "postcss": "^8.5.5",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  },
  "packageManager": "pnpm@10.12.3"
}
